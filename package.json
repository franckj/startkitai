  "scripts": {
    "setup": "node ./setup.js",
    "start": "NODE_ENV=production node ./index.js",
    "start:api": "NODE_ENV=production node ./server/index.js",
    "build": "NODE_ENV=production vite build ./web --outDir=../dist --emptyOutDir --config vite.config.js",
    "dev": "node ./index.js",
    "dev:api": "nodemon -w ./server --inspect=9228 --no-warnings server/index.js",
    "lint": "eslint ./web ./server",
    "spec": "speccy resolve ./server/docs/openapi.yaml > ./server/docs/openapi.build.yaml"
  },
  "imports": {
    "#ai/*": "./server/helpers/ai-providers/*",
    "#api/*": "./server/api/*",
    "#database/*": "./server/database/*",
    "#helpers/*": "./server/helpers/*",
    "#jobs/*": "./server/jobs/*",
    "#root/*": "./server/*"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
